  "description": "Tools related to connecting with Demo service",
  "categories": [ "Build and release" ],
  "Tags": [ "Release", "DevOps", "Artifacts" ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [ "contribution/ms.vss-releaseartifact.artifact-types" ],
  "icons": {
    "default": "images\\vss_default.png"
  },
  "branding": {
    "color": "#3B3E43",
    "theme": "dark"
  },
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "files": [
    {
      "path": "Tasks/DownloadDemoArtifacts"
    }
