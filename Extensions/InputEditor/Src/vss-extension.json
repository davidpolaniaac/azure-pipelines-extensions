{
    "manifestVersion": 1.0,
    "extensionId": "vss-distributed-task-input-editor",
    "name": "Task Input Editor",
    "version": "0.0.1",
    "publisher": "ms",
    "public": false,
    "description": "i18n:Editors to help with complex distributed task inputs",
    "categories": [ "Integrate" ],
    "galleryFlags": [
        "BuiltIn",
        "MultiVersion"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "vss_default.png",
        "wide": "vss_wide.png"
    },
    "files": [
        {
            "path": "UIExtensions/gridEditor.html", "addressable": true
        },
        {
            "path": "UIExtensions/css", "addressable": true
        },
        {
            "path": "UIExtensions/sdk", "addressable": true
        },
        {
            "path": "UIExtensions/scripts", "addressable": true
        }
    ],
    "eventCallbacks": {
        "versionCheck": {
            "uri": "{{DeploymentUrl \"00025394-6065-48CA-87D9-7F5672854EF7\"}}_apis/public/extensions/vdisc"
        }
    },
    "contributions": [
        {
            "id": "parameters-grid",
            "type": "ms.vss-distributed-task.task-input-editor",
            "targets": [ "ms.vss-distributed-task.task-input-editors" ],
            "properties": {
                "name":"Input Editor Parameter Grid",
                "uri": "UIExtensions/gridEditor.html",
                "order": "99"
            }
        }
    ]
}
