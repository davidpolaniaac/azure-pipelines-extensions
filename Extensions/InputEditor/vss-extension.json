{
    "manifestVersion": 1.0,
    "extensionId": "vss-service-distributed-task",
    "name": "Task Input Editor (Preview)",
    "version": "0.0.1",
    "publisher": "ms",
    "public": false,
    "description": "i18n:Complex inputs can be edited via this",
    "categories": [ "Integrate" ],
    "galleryFlags": [
        "BuiltIn"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "vss_default.png",
        "wide": "vss_wide.png"
    },
    "files": [
        {
            "path": "input_editor.html", "addressable": true
        },
        {
            "path": "css", "addressable": true
        },
        {
            "path": "sdk", "addressable": true
        },
        {
            "path": "scripts", "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "key-value-grid",
            "type": "ms.vss-distributed-task.task-input-editor",
            "targets": [ "ms.vss-distributed-task.task-input-editors" ],
            "properties": {
                "name":"Input Editor Grid",
                "uri": "input_editor.html",
                "order": "99"
            }
        }
    ]
}